<!DOCTYPE html>
<html>
<head>

    <!-- Basic Page Needs
    ================================================== -->
    <meta charset="utf-8">
    <title>國際縱火調查人員協會臺灣分會</title>

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Favicons
    ================================================== -->
    <link rel="shortcut icon" href="~/Scripts/HTML/Images/favicon.ico" type="image/x-icon" />

    <!-- CSS
    ================================================== -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/Scripts/HTML/css/bootstrap.min.css">
    <!-- Template styles-->
    <link rel="stylesheet" href="~/Scripts/HTML/css/style.css">
    <!-- Responsive styles-->
    <link rel="stylesheet" href="~/Scripts/HTML/css/responsive.css">
    <!-- FontAwesome -->
    <link rel="stylesheet" href="~/Scripts/HTML/css/font-awesome.min.css">
    <!-- Animation -->
    <link rel="stylesheet" href="~/Scripts/HTML/css/animate.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/Scripts/HTML/css/owl.carousel.css">
    <link rel="stylesheet" href="~/Scripts/HTML/css/owl.theme.css">
    <!-- Colorbox -->
    <link rel="stylesheet" href="~/Scripts/HTML/css/colorbox.css">

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements. All other JS at the end of file. -->
    <!--[if lt IE 9]>
      <script src="~/Scripts/HTML/js/html5shiv.js"></script>
      <script src="~/Scripts/HTML/js/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div class="body-inner">

        <!-- Header start -->
        <header id="header" class="header-solid">
            <div class="container">
                <div class="row">
                    <div class="logo col-md-6 col-sm-7 col-xs-12 ">
                        <a href="index.html">
                            <img src="~/Scripts/HTML/Images/logo.png" alt="國際縱火調查人員協會臺灣分會" title="國際縱火調查人員協會臺灣分會">
                        </a>
                    </div><!-- logo end -->

                    <div class="col-md-6 col-sm-5 col-xs-12 header-right hidden-xs">

                        <ul class="top-info">
                            <li>
                                <div class="info-box">
                                    <a href="https://www.facebook.com/pages/%E5%9C%8B%E9%9A%9B%E7%B8%B1%E7%81%AB%E8%AA%BF%E6%9F%A5%E4%BA%BA%E5%93%A1%E5%8D%94%E6%9C%83%E8%87%BA%E5%8F%B0%E7%81%A3%E5%88%86%E6%9C%83/469105899835805" title="粉絲專頁" target="_blank">
                                        <i class="fa fa-facebook">&nbsp;</i>
                                        <div class="info-box-content">
                                            <p class="info-box-title">粉絲專頁</p>
                                        </div>
                                    </a>
                                </div>
                            </li>
                            <li>
                                <div class="info-box">
                                    <a href="english/" title="English">
                                        <i class="fa fa-globe">&nbsp;</i>
                                        <div class="info-box-content">
                                            <p class="info-box-title">English</p>
                                        </div>
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </div><!-- header right end -->
                </div><!-- Row end -->
            </div><!-- Container end -->
        </header><!--/ Header end -->

        <nav class="site-navigation navigation">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="site-nav-inner">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                            <div class="collapse navbar-collapse navbar-responsive-collapse">

                                <ul class="nav navbar-nav">
                                    <li class="dropdown">
                                        <a href="about.html" class="dropdown-toggle" data-toggle="dropdown" title="關於我們">關於我們 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("關於我們", "AboutUs", "Article")</li>
                                            <li>@Html.ActionLink("組織架構", "Organization", "Article")</li>
                                            <li>@Html.ActionLink("沿革", "History", "Article")</li>
                                            <li>@Html.ActionLink("配證會員", "Member", "Article")</li>
                                            <li>@Html.ActionLink("專家介紹", "Expert", "Article")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="news.html" class="dropdown-toggle" data-toggle="dropdown" title="訊息發佈">訊息發佈 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("訊息發佈", "Index", "News")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="member.html" class="dropdown-toggle" data-toggle="dropdown" title="會員專區">會員專區 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("會員登入", "Login", "Member")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="business.html" class="dropdown-toggle" data-toggle="dropdown" title="協會業務">協會業務 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("協會業務", "Job", "Article")</li>
                                            <li>@Html.ActionLink("訓練、認證、發照", "Licenses", "Article")</li>
                                            <li>@Html.ActionLink("諮詢、顧問", "Refer", "Article")</li>
                                            <li>@Html.ActionLink("縱火調查", "Survey", "Article")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="knowledge.html" class="dropdown-toggle" data-toggle="dropdown" title="知識庫">知識庫 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("知識庫下載", "Index", "Knowledge")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="calendar.html" class="dropdown-toggle" data-toggle="dropdown" title="行事曆">行事曆 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("協會行事曆", "Calendar", "Article")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="contact.html" class="dropdown-toggle" data-toggle="dropdown" title="聯絡我們">聯絡我們 <i class="fa fa-angle-down"></i></a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>@Html.ActionLink("聯絡我們", "Index", "Contact")</li>
                                        </ul>
                                    </li>
                                </ul><!--/ Nav ul end -->

                            </div><!--/ Collapse end -->

                        </div><!-- Site Navbar inner end -->


                    </div><!--/ Col end -->
                </div><!--/ Row end -->
            </div><!--/ Container end -->
        </nav><!--/ Navigation end -->

        <div id="banner-area" class="banner-area" style="background-image: url(/Scripts/HTML/Images/banner-bg.jpg)">
            <!-- Subpage title start -->
            <div class="banner-text text-right">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12">
                            <ul class="breadcrumb">
                                <li>首頁</li>
                                @if (ViewBag.Breadcrumb != null)
                                {
                                    foreach (var item in ViewBag.Breadcrumb)
                                    {
                                        <li>
                                            @if (!string.IsNullOrEmpty(item.Url))
                                            {
                                                <a href="@item.Url">@item.Text</a>
                                            }
                                            else
                                            {
                                                @item.Text
                                            }
                                        </li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div><!-- Subpage title end -->
        </div><!-- Banner area end -->

        <section id="main-container" class="main-container">
            <div class="container">
                <div class="row">

                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">

                        <div class="sidebar sidebar-right">
                            <div class="widget">
                                <h2 class="widget-title"><i class="fa fa-folder-open-o">&nbsp;</i>訊息發佈</h2>
                                <ul class="arrow nav nav-tabs nav-stacked">
                                    <li>@Html.ActionLink("訊息發佈", "Index", "News")</li>
                                </ul>
                            </div><!-- Categories end -->

                        </div><!-- Sidebar end -->
                    </div><!-- Sidebar Col end -->

                    <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">

                        @*<h2 class="page-title">協會介紹</h2>
                            <hr class="botmargin-min">*@

                        <!-- 內容開始 -->

                        @RenderBody()

                        <!-- 內容結束 -->

                    </div><!-- Content Col end -->

                </div><!-- Main row end -->

            </div><!-- Conatiner end -->
        </section><!-- Main container end -->
        <!-- Footer start -->
        <footer id="footer" class="footer">
            <div class="container">

                <div class="row copyright">
                    <div class="col-xs-12 col-sm-7">
                        <div class="copyright-info">
                            <span>
                                國際縱火調查人員協會臺灣分會版權所有<br>
                                Copyright © 2016 International Association of Arson Investigators Taiwan Chapter. All Rights Reserved.
                            </span>
                        </div>
                    </div>

                    <div class="col-xs-12 col-sm-4 col-md-push-1">

                        <div class="footer-menu">
                            <a class="btn btn-dark" href="#" title="財產權宣告">財產權宣告</a>
                        </div>
                    </div>
                </div><!-- Copyright end -->

                <div id="back-to-top" data-spy="affix" data-offset-top="10" class="back-to-top affix">
                    <button class="btn btn-primary" title="Back to Top">
                        <i class="fa fa-angle-double-up"></i>
                    </button>
                </div>

            </div><!--/ Container end -->
        </footer><!-- Footer end -->
        <!-- Javascript Files
        ================================================== -->
        <!-- initialize jQuery Library -->
        <script type="text/javascript" src="~/Scripts/HTML/js/jquery.js"></script>
        <!-- Bootstrap jQuery -->
        <script type="text/javascript" src="~/Scripts/HTML/js/bootstrap.min.js"></script>
        <!-- Owl Carousel -->
        <script type="text/javascript" src="~/Scripts/HTML/js/owl.carousel.min.js"></script>
        <!-- Counter -->
        <script type="text/javascript" src="~/Scripts/HTML/js/jquery.counterup.min.js"></script>
        <!-- Waypoints -->
        <script type="text/javascript" src="~/Scripts/HTML/js/waypoints.min.js"></script>
        <!-- Color box -->
        <script type="text/javascript" src="~/Scripts/HTML/js/jquery.colorbox.js"></script>
        <!-- Isotope -->
        <script type="text/javascript" src="~/Scripts/HTML/js/isotope.js"></script>
        <script type="text/javascript" src="~/Scripts/HTML/js/ini.isotope.js"></script>
        <!-- Google Map API Key Source -->
        <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
        <!-- For Google Map -->
        <!-- Doc http://www.mkyong.com/google-maps/google-maps-api-hello-world-example/ -->
        <script type="text/javascript" src="~/Scripts/HTML/js/gmap3.min.js"></script>
        <!-- Template custom -->
        <script type="text/javascript" src="~/Scripts/HTML/js/custom.js"></script>

    </div><!-- Body inner end -->
</body>
</html>
